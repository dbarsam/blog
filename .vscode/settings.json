{
    "files.trimTrailingWhitespace": true,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.exclude": {
        "**/*.pyc": true,
        "**/__pycache__": true,
    },
    "cSpell.words": [
        "backticks",
        "monospace",
        "submodule",
        "submodules",
        "symlink"
    ],
}
